{
  "codeLens.enable": true,
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "eelixir",
    "css",
    "Markdown",
    "json",
    "javascriptreact",
    "ruby",
    "typescript",
    "typescript.tsx",
    "html",
    "scss"
  ],
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["vim", "eruby", "markdown"]
    },
    "ember-language-server": {
      "module": "/usr/local/lib/node_modules/@emberwatch/ember-language-server/lib/start-server.js",
      "args": ["stdio"],
      "filetypes": ["html.handlebars", "javascript", "javascript.jsx"],
      "rootPatterns": [".ember-cli"],
      "requireRootPattern": true
    }
  }
}
